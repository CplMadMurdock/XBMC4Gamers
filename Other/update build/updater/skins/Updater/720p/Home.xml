<window id="0">
		<defaultcontrol always="true">1</defaultcontrol>
		<allowoverlay>no</allowoverlay>
		<controls>
				<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>1280</width>
						<height>720</height>
						<texture fallback="Background.png">$INFO[Skin.String(Background_Color)]</texture>
						<aspectratio>stretch</aspectratio>
				</control>
				<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>1280</width>
						<height>720</height>
						<texture>background diffuse.png</texture>
						<colordiffuse>$VAR[var-bgcustcolour]</colordiffuse>
						<aspectratio>stretch</aspectratio>
				</control>
				<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>1280</width>
						<height>720</height>
						<texture>$INFO[Skin.String(Background_Image)]</texture>
						<aspectratio>scale</aspectratio>
				</control>
				<!-- Debug Info -->
				<include>Debug</include>
		</controls>
</window>