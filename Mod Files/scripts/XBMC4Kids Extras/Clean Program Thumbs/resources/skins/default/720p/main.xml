<window id="129">
		<defaultcontrol always="false">3000</defaultcontrol>
		<coordinates>
				<system>1</system>
				<posx>460</posx>
				<posy>250</posy>
		</coordinates>
		<controls>
				<control type="image">
						<description>underlay image</description>
						<posx>-460</posx>
						<posy>-250</posy>
						<width>1280</width>
						<height>720</height>
						<texture border="5">main-underlay.png</texture>
						<animation effect="fade" start="0" end="100" time="200">WindowOpen</animation>
						<animation effect="fade" start="100" end="0" time="200">WindowClose</animation>
				</control>
				<control type="group">
						<animation effect="fade" start="0" end="100" time="500">WindowOpen</animation>
						<animation effect="fade" start="100" end="0" time="500">WindowClose</animation>
						<control type="image">
								<description>background image</description>
								<posx>0</posx>
								<posy>0</posy>
								<width>350</width>
								<height>220</height>
								<texture border="5">main-background.png</texture>
						</control>
						<control type="image">
								<description>Heading Background</description>
								<posx>0</posx>
								<posy>0</posy>
								<width>350</width>
								<height>40</height>
								<texture border="5">main-header.png</texture>
						</control>
						<control type="label">
								<description>heading label</description>
								<posx>0</posx>
								<posy>0</posy>
								<width>350</width>
								<height>40</height>
								<align>center</align>
								<aligny>center</aligny>
								<font>font18</font>
								<label>Clean Program Thumbs</label>
								<textcolor>black</textcolor>
						</control>
						<control type="grouplist">
								<description>custom grouplist for context buttons</description>
								<posx>25</posx>
								<posy>65</posy>
								<width>350</width>
								<height>300</height>
								<itemgap>0</itemgap>
								<control type="button" id="3000">
										<description>Action Button</description>
										<font>font12</font>
										<align>center</align>
										<textcolor>white</textcolor>
										<label>Clean Thumbs</label>
										<texturefocus border="3">main-button-focus.png</texturefocus>
										<texturenofocus border="3">main-button-nofocus.png</texturenofocus>
								</control>
								<control type="button" id="3001">
										<description>Action Button</description>
										<font>font12</font>
										<align>center</align>
										<textcolor>white</textcolor>
										<label>Generate Thumbs</label>
										<texturefocus border="3">main-button-focus.png</texturefocus>
										<texturenofocus border="3">main-button-nofocus.png</texturenofocus>
								</control>
								<control type="button" id="3002">
										<description>Action Button</description>
										<font>font12</font>
										<align>center</align>
										<textcolor>white</textcolor>
										<label>Remove Thumbs</label>
										<texturefocus border="3">main-button-focus.png</texturefocus>
										<texturenofocus border="3">main-button-nofocus.png</texturenofocus>
								</control>
						</control>
				</control>
		</controls>
</window>