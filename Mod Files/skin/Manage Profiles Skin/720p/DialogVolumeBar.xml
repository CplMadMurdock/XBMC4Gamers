<window id="104">
		<defaultcontrol>1</defaultcontrol>
		<include>dialogeffect</include>
		<coordinates>
				<system>1</system>
				<posx>860</posx>
				<posy>0</posy>
		</coordinates>
		<controls>
				<control type="group">
						<visible>!StringCompare(System.Profilename,Manage Profiles) + !StringCompare(System.Profilename,DVD2Xbox)</visible>
						<control type="image">
								<posx>5</posx>
								<posy>5</posy>
								<width>395</width>
								<height>35</height>
								<texture>black-back.png</texture>
						</control>
						<control type="label">
								<description>Volume Label</description>
								<posx>90</posx>
								<posy>5</posy>
								<width>90</width>
								<height>35</height>
								<label>$INFO[Player.Volume]</label>
								<font>font12</font>
								<align>right</align>
								<aligny>center</aligny>
						</control>
						<control type="progress" id="1">
								<description>Progressbar</description>
								<posx>100</posx>
								<posy>16</posy>
								<width>250</width>
								<height>12</height>
								<info>Player.Volume</info>
								<visible>true</visible>
						</control>
						<control type="image">
								<posx>360</posx>
								<posy>5</posy>
								<width>35</width>
								<height>35</height>
								<texture>icon_volume.png</texture>
						</control>
				</control>
				<!-- Debug Info -->
				<include>Debug</include>
		</controls>
</window>