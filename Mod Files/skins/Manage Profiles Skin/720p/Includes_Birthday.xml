<includes>
		<include name="birthday">
				<!-- Birthday -->
				<control type="image">
						<posx>440</posx>
						<posy>50</posy>
						<width>400</width>
						<height>180</height>
						<texture>birthday\happy_birthday_banner.png</texture>
						<aspectratio>stretch</aspectratio>
						<animation type="Conditional" condition="true" reversible="true" loop="true">
								<effect type="zoom" tween="sine" easing="out" start="0,0,400,180" end="0,0,440,190" center="auto" delay="5000" time="500" />
								<effect type="zoom" tween="sine" easing="out" start="0,0,440,190" end="0,0,400,180" center="auto" delay="5500" time="500" />
								<effect type="zoom" tween="sine" easing="out" start="0,0,400,180" end="0,0,440,190" center="auto" delay="6000" time="500" />
								<effect type="zoom" tween="sine" easing="out" start="0,0,440,190" end="0,0,400,180" center="auto" delay="6500" time="500" />
								<effect type="zoom" tween="sine" easing="out" start="0,0,400,180" end="0,0,440,190" center="auto" delay="7000" time="500" />
								<effect type="zoom" tween="sine" easing="out" start="0,0,440,190" end="0,0,400,180" center="auto" delay="7500" time="500" />
						</animation>
				</control>
				<control type="image">
						<posx>0</posx>
						<posy>340</posy>
						<width>295</width>
						<height>436</height>
						<texture>birthday\balloons.png</texture>
						<aspectratio>stretch</aspectratio>
						<animation type="Conditional" condition="true" reversible="true" loop="true">
								<effect type="slide" tween="sine" easing="inout" start="0,0" end="0,-40" time="2000" />
								<effect type="slide" tween="sine" easing="inout" start="0,0" end="0,40" delay="2000" time="2000" />
						</animation>
				</control>
				<control type="image">
						<posx>985</posx>
						<posy>340</posy>
						<width>295</width>
						<height>436</height>
						<texture flipx="true">birthday\balloons.png</texture>
						<aspectratio>stretch</aspectratio>
						<animation type="Conditional" condition="true" reversible="true" loop="true">
								<effect type="slide" tween="sine" easing="inout" start="0,0" end="0,-40" time="2000" />
								<effect type="slide" tween="sine" easing="inout" start="0,0" end="0,40" delay="2000" time="2000" />
						</animation>
				</control>
		</include>
</includes>