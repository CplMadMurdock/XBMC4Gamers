<window id="12">
		<defaultcontrol>3</defaultcontrol>
		<allowoverlay>no</allowoverlay>
		<controls>
				<include>CommonBackground</include>
				<control type="group">
						<include>ViewVisibleEffect</include>
						<posx>20</posx>
						<posy>60</posy>
						<control type="image">
								<description>background image</description>
								<posx>0</posx>
								<posy>0</posy>
								<width>300</width>
								<height>600</height>
								<texture>black-back.png</texture>
						</control>
						<control type="grouplist" id="3">
								<description>button area</description>
								<posx>0</posx>
								<posy>30</posy>
								<width>300</width>
								<height>540</height>
								<itemgap>10</itemgap>
								<onleft>5</onleft>
								<onright>5</onright>
								<onup>3</onup>
								<ondown>3</ondown>
						</control>
				</control>
				<control type="group">
						<include>ViewVisibleEffect</include>
						<control type="image">
								<description>background image</description>
								<posx>321</posx>
								<posy>60</posy>
								<width>949</width>
								<height>600</height>
								<texture>black-back.png</texture>
						</control>
						<control type="grouplist" id="5">
								<description>control area</description>
								<posx>321</posx>
								<posy>90</posy>
								<width>949</width>
								<height>540</height>
								<itemgap>0</itemgap>
								<pagecontrol>90</pagecontrol>
								<onleft>3</onleft>
								<onright>90</onright>
								<onup>5</onup>
								<ondown>5</ondown>
						</control>
						<control type="scrollbar" id="90">
								<posx>1270</posx>
								<posy>90</posy>
								<width>10</width>
								<height>560</height>
								<texturesliderbackground border="2,2,2,2">scrollbar_bar_back.png</texturesliderbackground>
								<texturesliderbar border="2,16,2,16">scrollbar_bar.png</texturesliderbar>
								<texturesliderbarfocus border="2,16,2,16">scrollbar_bar_focus.png</texturesliderbarfocus>
								<textureslidernib>scrollbar_nib.png</textureslidernib>
								<textureslidernibfocus>scrollbar_nib.png</textureslidernibfocus>
								<onleft>5</onleft>
								<onright>3</onright>
								<showonepage>false</showonepage>
								<orientation>vertical</orientation>
						</control>
				</control>
				<control type="button" id="10">
						<include>ButtonSettingsCategoryValues</include>
				</control>
				<control type="button" id="7">
						<description>Default Button</description>
						<posx>0</posx>
						<posy>0</posy>
						<height>50</height>
						<font>size_24</font>
						<textcolor>white</textcolor>
						<focusedcolor>black</focusedcolor>
						<aligny>center</aligny>
						<textoffsetx>20</textoffsetx>
						<texturefocus border="5">ThumbFO.png</texturefocus>
						<texturenofocus border="5">-</texturenofocus>
				</control>
				<control type="radiobutton" id="8">
						<description>Default RadioButton</description>
						<posx>0</posx>
						<posy>0</posy>
						<height>50</height>
						<font>size_24</font>
						<textcolor>white</textcolor>
						<focusedcolor>black</focusedcolor>
						<textoffsetx>20</textoffsetx>
						<radiowidth>73</radiowidth>
						<radioheight>25</radioheight>
						<texturefocus border="5">ThumbFO.png</texturefocus>
						<texturenofocus border="5">-</texturenofocus>
				</control>
				<control type="spincontrolex" id="9">
						<description>Default SpinControlex</description>
						<posx>0</posx>
						<posy>0</posy>
						<height>50</height>
						<font>size_24</font>
						<textcolor>white</textcolor>
						<focusedcolor>black</focusedcolor>
						<textoffsetx>20</textoffsetx>
						<texturefocus border="5">ThumbFO.png</texturefocus>
						<texturenofocus border="5">-</texturenofocus>
						<aligny>center</aligny>
						<reverse>yes</reverse>
				</control>
				<control type="image" id="11">
						<description>separator image</description>
						<height>1</height>
						<texture>-</texture>
				</control>
				<control type="image">
						<posx>20</posx>
						<posy>5</posy>
						<width>40</width>
						<height>35</height>
						<aspectratio>keep</aspectratio>
						<texture>icon_system.png</texture>
						<include>Window_OpenClose_Animation</include>
				</control>
				<control type="label">
						<description>Header label</description>
						<posx>70</posx>
						<posy>5</posy>
						<width>700</width>
						<height>35</height>
						<align>left</align>
						<aligny>center</aligny>
						<font>size_23</font>
						<textcolor>white</textcolor>
						<label>$LOCALIZE[5] - $INFO[Control.GetLabel(2)]</label>
						<include>Window_OpenClose_Animation</include>
				</control>
				<control type="label" id="2">
						<description>Fake Label so we can pass it value down to the one above</description>
						<posx>0</posx>
						<posy>0</posy>
						<width>0</width>
						<height>0</height>
						<font>-</font>
						<visible>false</visible>
				</control>
				<include>WindowHeader</include>
				<include>WindowFooter</include>
				<!-- Debug Info -->
				<include>Debug</include>
		</controls>
</window>